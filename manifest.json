{
	"author": "Will Huang",
	"name": "Watch Video Online",
	"description": "Modify the response header to avoid video download as attachment",
	"version": "0.0.1",
	"manifest_version": 2,
	"homepage_url": "https://github.com/doggy8088/WatchVideoOnlineChromeExtension/",
	"background": {
		"scripts": ["watch-video-online.js"],
		"persistent": true
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	]
}
